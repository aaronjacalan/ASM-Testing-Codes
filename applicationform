    	mov ah, 06h
    	mov ch, 12
    	mov cl, 35 
    	mov dh, 12
    	mov dl, 44 
    	mov bh, 04Eh
    	int 10h
	
	mov ah, 06h
    	mov ch, 14 
    	mov cl, 2 
    	mov dh, 14 
    	mov dl, 77
    	mov bh, 070h
    	int 10h

    	mov ah, 06h
    	mov ch, 17   ; Row start
    	mov cl, 2   ; Column start
    	mov dh, 17   ; Row end
    	mov dl, 77  ; Column end
    	mov bh, 08Eh ; Yellow on yellow (blinking)
    	int 10h


    	; Display the form
    	mov ah, 09h
    	lea dx, disp
    	int 21h


    	; Exit program
    	mov ax, 4C00h
    	int 21h

main endp

end main
