.code
main proc
    	mov ax, @data
    	mov ds, ax


    	; Clear screen
    	mov ax, 0003h
    	int 10h


    	; Header grey background
    	mov ah, 06h
    	mov al, 0   ; Clear entire area
    	mov ch, 1   ; Row start
    	mov cl, 2   ; Column start
    	mov dh, 3   ; Row end
    	mov dl, 77  ; Column end
    	mov bh, 70h ; Grey background
    	int 10h

	mov ah, 06h
    	xor al, al
    	xor cx, cx
    	mov cl, 3
    	mov ch, 1
    	mov dl, 76
    	mov bh, 4Fh
    	int 10h

    	; BLINKING TEXT
    	mov ah, 06h
    	mov al, 0
    	mov ch, 3   ; Row start
    	mov cl, 3   ; Column start
    	mov dh, 3   ; Row end
    	mov dl, 76  ; Column end
    	mov bh, 0CEh ; Yellow on yellow (blinking)
    	int 10h


    	mov ah, 06h
    	mov ch, 4
    	mov cl, 2
    	mov dh, 4
    	mov dl, 77
    	mov bh, 7Fh
    	int 10h


	; ROW 1 WITH BUTTONS
    	mov ah, 06h
    	mov ch, 5; row start
    	mov cl, 2; col start
    	mov dh, 5; row end
    	mov dl, 28 ; col end
    	mov bh, 070h
    	int 10h

    	mov ah, 06h
    	mov ch, 5; row start
    	mov cl, 41; col start
    	mov dh, 5; row end
    	mov dl, 65 ; col end
    	mov bh, 070h
    	int 10h

    	mov ah, 06h
    	mov ch, 5; row start
    	mov cl, 77; col start
    	mov dh, 5; row end
    	mov dl, 77 ; col end
    	mov bh, 070h
    	int 10h

	;ROW 1 BUTTONS
	mov ah, 06h
    	mov ch, 5
    	mov cl, 38
    	mov dh, 5
    	mov dl, 40
    	mov bh, 4Fh
    	int 10h

	mov ah, 06h
    	mov ch, 5
    	mov cl, 74
    	mov dh, 5
    	mov dl, 76
    	mov bh, 4Fh
    	int 10h


	;ROW 2 WITHOUT BUTTONS
